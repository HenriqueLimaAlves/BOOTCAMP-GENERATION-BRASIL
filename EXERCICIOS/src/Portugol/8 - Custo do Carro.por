programa
{
	inclua biblioteca Matematica --> mat
	funcao inicio()
	{
		real custoF, custoC

		escreva("Digite o custo de fabrica: ")
		leia(custoF)

		custoC= custoF + (custoF*0.73)

		escreva("\nO valor final do carro é de: ", custoC)
	}
}

